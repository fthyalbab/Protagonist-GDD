        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// set volume
// for every channel
for (var i = 0; i &lt; ds_list_size(sounds); i++)
{
    // every sound's gain is set to match its channel
    var channel = sounds[| i];
    for (var j = 0; j &lt; ds_list_size(channel); j++)
    {
        var sound = channel[| j];
        // if playing set volume
        if (audio_is_playing(sound))
        {
            audio_sound_gain(sound, volume[| i] * fade[| i], 0);
        }
        // remove if no longer playing
        else
        {
            ds_list_delete(channel, j);
            j--;
        }
    }
}
</string>
