          <argument>
            <kind>1</kind>
            <string>/// tear animation

// position is at target
x = targetX;
y = targetY;

switch(state)
{
    case OPENING:
        image_alpha = 1;
        // tear animation fills
        if (floor(image_index) &gt; 3)
        {
            state = OPEN;
            timer = 0;
            // deal damage here
            combatEndCast(targetX, targetY, action);
            combatLastCast(targetX, targetY, action);
            combatDamage(targetX, targetY, 15);
        }
        break;
    // loop tear animation for a while
    case OPEN:
        image_alpha = 1;
        timer++;
        if (timer &gt; openDur)
        {
            state = CLOSING;
            timer = 0;
        }
        break;
    // fade away
    case CLOSING:
        image_alpha = map_range(timer, 0, closeDur, 1, 0);
        timer++;
        if (timer &gt; closeDur)
        {
            state = CLOSED;
            timer = 0;
        }
        break;
    // destroy self when it's gone
    case CLOSED:
        instance_destroy();
        break;
}
</string>
          </argument>
        </arguments>
      </action>
