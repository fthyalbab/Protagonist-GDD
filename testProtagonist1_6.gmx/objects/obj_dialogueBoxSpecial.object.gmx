            <kind>1</kind>
            <string>/// special on
// starts dialogue with the special effect of dim and fade.
// Converts to special effect dialogue if already started
dimmer = screenDimmer(screenDim, screenDur);
masterVolumeFader(MUSIC, volumeFade, volumeDur);
// play the starting ding
playSound(SFX, specialStartSound);
// deactivate the combat objects and save image to surface
if (checkCombatActive())
{
    if (!surface_exists(surf))
    {
        surf = surface_create(getDisplayRoomWidth(), getDisplayRoomHeight());
    }
    surface_copy(surf, 0, 0, application_surface);
    surface_set_target(surf);
    var this = id;
    with (all)
    {
        if (id != this &amp;&amp; !instanceof(id, obj_drawer))
        {
            event_perform(ev_draw, ev_gui);
        }
    }
    surface_reset_target();
    // deactivate all other things
    instance_deactivate_all(true);
    reactivateControlObjects();
    instance_activate_object(obj_drawer);
    instance_activate_object(obj_screenDimmer);
    instance_activate_object(obj_masterVolumeFader);
}
</string>
