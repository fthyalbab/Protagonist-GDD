            <string>/// create letters

if (i &gt;= string_length(str))
{
    exit;
}

timer += 1 / delay;

for (timer = timer; timer &gt;= 1; timer--)
{
    // generate letter, if between a and z
    var char = string_char_at(str, i + 1);
    if (inrange(ord(char), ord('a'), ord('z') + 1))
    {
        var obj = createLetter(x + (i * (sprite_get_width(spr_Spells) * scale)), y, char);
        obj.depth = object_get_depth(obj.object_index) + 1;
        obj.spell = false;
        obj.str = str;
        obj.pos = i;
        obj.image_xscale = scale;
        obj.image_yscale = scale;
        if (!anim)
        {
            obj.normalMin = 5;
            obj.normalMax = 140;
            obj.greekMin = 5;
            obj.greekMax = 15;
        }
        if (ds_exists(list, ds_type_list))
        {
            ds_list_add(list, obj);
        }
    }
    
    // increment
    i++;
    
    // bounds check
    if (i &gt;= string_length(str))
    {
        exit;
    }
}
</string>
