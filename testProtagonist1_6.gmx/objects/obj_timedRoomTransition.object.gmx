        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// state machine
switch(state)
{
    case CLOSED:
        // inactive
        timer = 0;
        break;
    case OPENING:
        timer += openSpeed;
        if (timer &gt;= duration)
        {
            if (pause)
            {
                state = OPEN;
                pauseTimer = 0;
            }
            else
            {
                state = CLOSING;
                // transition room
                if (room_exists(target))
                {
                    room = target;
                }
            }
        }
        break;
    case OPEN:
        if (pause)
        {
            if (pauseTimer &lt; pauseDur)
            {
                pauseTimer++;
            }
        }
        else
        {
            pauseTimer--;
            if (pauseTimer &lt;= 0)
            {
                state = CLOSING;
                // transition room
                if (room_exists(target))
                {
                    room = target;
                }
            }
        }
        break;
    case CLOSING:
        timer -= closeSpeed;
        if (timer &lt; 0)
        {
            state = CLOSED;
            // when finished, destroy
            instance_destroy();
        }
        break;
    default:
        show_error("Invalid room transition state: " + string(state), true);
        state = CLOSED;
        break;
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
