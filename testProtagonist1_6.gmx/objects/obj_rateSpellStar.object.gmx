          <argument>
            <kind>1</kind>
            <string>/// init star that is thrown in the rate spell

// target data stored here
targetX = 0;
targetY = 0;
action = "";

// keeps track of first and last hit, set by obj_rateSpell
first = false;
last = false;

// three stages
// initially, wait for a bit (CLOSED)
// first, start spinning faster and faster (OPENING)
state = CLOSED;
// then move quickly toward target (OPEN)
// lastly, explode

spinSpd = 0;
spinAcc = 2.2;
spinMax = 23 + irandom_range(-4, 4);

spd = 34;

// wait
waitDur = 35;
spinDur = 12;
timer = 0;

image_speed = 0.3;

</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// handle state

switch (state)
{
    // initially, wait for a bit
    case CLOSED:
        timer++;
        if (timer &gt;= waitDur)
        {
            state = OPENING;
            timer = 0;
        }
        break;
    // first, start spinning faster and faster
    case OPENING:
        timer++;
        spinSpd += spinAcc;
        image_angle += spinSpd;
        if (timer &gt;= spinDur)
        {
            state = OPEN;
            timer = 0;
        }
        break;
    // then move quickly toward target
    case OPEN:
        image_angle += spinSpd;
        // if hit
        if (point_distance(x, y, targetX, targetY) &lt; spd)
        {
            x = targetX;
            y = targetY;
            // create explosion
            effect_create_above(ef_explosion, x, y, 1, make_color_rgb(238, 188, 53));
            instance_destroy();
            // handle first/last and damage
            combatDamage(targetX, targetY, 8);
            if (first)
            {
                combatEndCast(targetX, targetY, action);
            }
            if (last)
            {
                combatLastCast(targetX, targetY, action);
            }
            exit;
        }
        // otherwise, move toward
        else
        {
            var dir = point_direction(x, y, targetX, targetY);
            x += lengthdir_x(spd, dir);
            y += lengthdir_y(spd, dir);
        }
        break;
}
</string>
