            <string>/// savvy's magic button that loads a dialogue file
var B = 5;
var w = sprite_get_width(spr_savvysMagicButton);
var h = sprite_get_height(spr_savvysMagicButton);
// drwa the button
draw_sprite_ext(spr_savvysMagicButton, 0, B, B, 1, 1, 0, c_white, 1);
if (mouse_in_rect(B, B, w, h))
{
    // if mouseover, then highlight the button
    // it calls to you: "press the button... press the button..."
    draw_set_alpha(0.4);
    draw_set_color(c_white);
    draw_rectangle(B + 1, B + 1, B + w - 2, B + h - 2, false);
    // if mouse pressed, load the file
    if (mouse_check_button_pressed(mb_left))
    {
        var f = get_open_filename_ext("Text file|*.*", "", working_directory, "Open this dialogue file...");
        if (f != "" &amp;&amp; file_exists(f))
        {
            // load and start the dialogue
            dialogueLoad(f);
            dialogueStart();
        }
    }
}
