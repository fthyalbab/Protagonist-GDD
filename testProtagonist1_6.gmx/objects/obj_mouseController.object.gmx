            <kind>1</kind>
            <string>/// move the cursor

if (size == noone)
{
    size = point_distance(cursorX, cursorY, targetX, targetY);
}
else
{
    size -= max(0, 0.5 * spd);
}

var dir = point_direction(cursorX, cursorY, targetX, targetY)
if (point_distance(cursorX, cursorY, targetX, targetY) &gt; spd || size &lt; spd * 0.5)
{
    cursorX += lengthdir_x(spd, dir);
    cursorY += lengthdir_y(spd, dir);
    if (point_distance(cursorX, cursorY, targetX, targetY) &gt; size)
    {
        cursorX = targetX + lengthdir_x(size, dir + 180);
        cursorY = targetY + lengthdir_y(size, dir + 180);
    }
}
else
{
    cursorX = targetX;
    cursorY = targetY;
    display_mouse_set(targetX, targetY);
    if (endGame)
    {
        game_end();
    }
}
cursor_set_pos(cursorX, cursorY);
</string>
